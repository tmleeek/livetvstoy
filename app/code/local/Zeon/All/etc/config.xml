<config>
    <modules>
        <Zeon_All>
            <version>0.1.0</version>
        </Zeon_All>
    </modules>
    <global>
        <models>
            <zeon_all>
                <class>Zeon_All_Model</class>
            </zeon_all>
        </models>
        <helpers>
            <zeon_all>
                <class>Zeon_All_Helper</class>
            </zeon_all>
        </helpers>
    </global>
    <adminhtml>
        <cms>
            <browser>
                <extensions>
                    <allowed>
                        <jpg>1</jpg>
                        <jpeg>1</jpeg>
                        <png>1</png>
                        <gif>1</gif>
                        <pdf>1</pdf>
                    </allowed>
                    <media_allowed>
                        <flv>1</flv>
                        <swf>1</swf>
                        <avi>1</avi>
                        <mov>1</mov>
                        <rm>1</rm>
                        <wmv>1</wmv>
                        <pdf>1</pdf>
                    </media_allowed>
                </extensions>
            </browser>
        </cms>
    </adminhtml>
</config>